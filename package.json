{
  "name": "happynrwl",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "create:ts:tsc": "nx generate @nrwl/js:library --name=hello-tsc --buildable",
    "create:ts:swc": "nx generate @nrwl/js:library --name=hello-swc --buildable --compiler=swc",
    "convert:to:swc": "nx generate @nrwl/js:convert-to-swc --name=hello-tsc",
    "create:ts:swc:npm": "nx generate @nrwl/js:library --name=hello-tsc-npm --buildable --config=npm-scripts",
    "build:all": "nx run-many --target=build --all",
    "lint:all": "nx run-many --target=lint --all",
    "test:all": "nx run-many --target=test --all"
  },
  "private": true,
  "dependencies": {
    "@swc/helpers": "~0.3.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.5.8",
    "@nrwl/eslint-plugin-nx": "14.5.8",
    "@nrwl/jest": "14.5.8",
    "@nrwl/js": "14.5.8",
    "@nrwl/linter": "14.5.8",
    "@nrwl/node": "^14.5.8",
    "@nrwl/workspace": "14.5.8",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@swc/jest": "0.2.20",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
    "nx": "14.5.8",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  }
}
